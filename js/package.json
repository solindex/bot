    "@types/node": "^14.14.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.17.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "save-dev": "0.0.1-security",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@dr497/awesome-serum-markets": "^1.1.41",
    "@project-serum/serum": "^0.13.37",
