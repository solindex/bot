{
  "name": "@solindex/bot",
  "version": "0.5.3",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/test.ts",
    "build": "tsc",
    "clean": "rm -r dist",
    "prepublish": "tsc",
    "lint": "yarn pretty && eslint .",
